<nav aria-label="Page navigation example">
        <ul class="pagination">
        <% if(pages > 0 ) { %>
            <% if (Number(current) === 1) { %>
                <li class="disabled"><a class="page-link">First</a></li>
            <% } else { %>
                <li><a href="/user?page=1" class="page-link">First</a></li>
            <% } %>
            <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
            <% if(i !== 1) { %>
                <li>
                        <li class="page-item disabled"><a class="page-link" href="#">...</a></li>
                    </li>
            <% } %>
          <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
            <% if (i === Number(current)) { %>
                <li>
                    <li class="page-item disabled"><a class="page-link" href="/user?page=<%= i %>"><%= i %></a></li>
                </li>
            <% } else { %> 
                <li>
                    <li class="page-item"><a class="page-link" href="/user?page=<%= i %>"><%= i %></a></li>
                </li>
            <% } %>
            <% if (i === Number(current) + 4 && i < pages) { %>
                <li class="page-item disabled"><a class="page-link">...</a></li>
            <% } %>
          <% } %>
          <% if (Number(current) === pages) { %>
                <li class="page-item disabled"><a class="page-link">Last</a></li>
            <% } else { %>
                <li><a href="/user?page=<%= pages %>" class="page-link">Last</a></li>
            <% } %>
        <% } %>
        </ul>
      </nav>